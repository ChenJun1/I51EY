<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/stowage_layout"
    android:orientation="vertical">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:gravity="center"
        android:background="?android:attr/selectableItemBackground"
        android:orientation="horizontal">

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.5"
            android:orientation="vertical">

            <View
                android:id="@+id/view"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentTop="true"
                android:background="#f4f4f8"
                android:visibility="gone"/>

            <TextView
                android:id="@+id/Operate_DateTime"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="05-07"
                android:textSize="12sp"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/weiqianshou"
                    android:layout_width="60dp"
                    android:layout_height="25dp"
                    android:layout_gravity="center_vertical|right"
                    android:background="@drawable/un_finish"
                    android:layout_marginRight="100dp"
                    android:layout_marginTop="@dimen/dimen_10"
                    android:gravity="center"
                    android:text="未到车"
                    android:textColor="#ffffff"
                    android:textSize="12sp"
                    android:visibility="gone"/>

                <View
                    android:id="@+id/views"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_alignParentTop="true"
                    android:background="#f4f4f8"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:gravity="center"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="25dp"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <Button
                                android:id="@+id/Traffic_Mode_btn"
                                android:layout_width="14dp"
                                android:layout_height="14dp"
                                android:background="@drawable/shape_rounded_btn_8ddc8c"
                                android:text="整"
                                android:textColor="#ffffff"
                                android:textSize="@dimen/font_10" />

                            <TextView
                                android:id="@+id/CarCode_text"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:gravity="center_vertical"
                                android:text="--"
                                android:layout_marginLeft="@dimen/dimen_5"
                                android:textColor="#ff000000"
                                android:textSize="@dimen/font_14" />
                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="25dp"
                            android:gravity="center_vertical"
                            android:layout_weight="1">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/stowage_list_item_gua"
                                android:textColor="#818181"
                                android:textSize="@dimen/font_14" />

                            <TextView
                                android:id="@+id/BoxCarCode_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dimen_5"
                                android:text="--"
                                android:textColor="#818181"
                                android:textSize="@dimen/font_14" />
                            android:textSize="@dimen/font_14" />


                        </LinearLayout>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:gravity="center_vertical">

                        <Button
                            android:id="@+id/Line_Oid_btn"
                            android:layout_width="@dimen/dimen_14"
                            android:layout_height="@dimen/dimen_14"
                            android:background="@drawable/shape_rounded_btn_f56164"
                            android:text="支"
                            android:textColor="#ffffff"
                            android:textSize="@dimen/font_10"

                            />

                        <TextView
                            android:id="@+id/Line_Name_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dimen_5"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:text="--"
                            android:textColor="#000000" />

                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="25dp"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@drawable/shape_rounded_tv_f5bb4e_3"
                                android:paddingLeft="@dimen/dimen_2"
                                android:paddingRight="@dimen/dimen_2"
                                android:gravity="center_vertical"
                                android:text="发"
                                android:textColor="#FFFFFF"
                                android:textSize="12sp"/>

                            <TextView
                                android:id="@+id/sendCity_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="5dp"
                                android:gravity="center_vertical"
                                android:text="--"
                                android:textSize="12sp"/>
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/end_time_text_layout"
                            android:layout_width="0dp"
                            android:layout_height="25dp"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@drawable/shape_rounded_tv_8dc15a_3"
                                android:gravity="center"
                                android:paddingLeft="@dimen/dimen_2"
                                android:paddingRight="@dimen/dimen_2"
                                android:text="到"
                                android:textColor="#FFFFFF"
                                android:textSize="12sp"/>

                            <TextView
                                android:id="@+id/arriveCity_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="5dp"
                                android:gravity="center_vertical"
                                android:text="--"
                                android:textSize="12sp"/>
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/stowage_list_item_size"
                                android:textColor="#818181"
                                android:textSize="@dimen/font_14" />

                            <TextView
                                android:id="@+id/Ticket_Count_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dimen_5"
                                android:text="--"
                                android:textColor="#818181"
                                android:textSize="@dimen/font_14" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="horizontal">


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/stowage_list_item_profit"
                                android:textColor="#818181"
                                android:textSize="@dimen/font_14" />

                            <TextView
                                android:id="@+id/Profit_Fee_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dimen_5"
                                android:text="--"
                                android:textColor="#818181"
                                android:textSize="@dimen/font_14" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
        </RelativeLayout>
    </LinearLayout>













